<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>Name</th>
            <th>password</th>
        </tr>
    </thead>
    <tbody>
        <% for (let i = 0; i < users.length; i++) { %>
            <tr>
                <td><%= users[i].id %></td>
                <td><%= users[i].email %></td>
                <td><%= users[i].name %></td>
                <td><%= users[i].password %></td>
            </tr>
        <% } %>
    </tbody>
</table>